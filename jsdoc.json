{
    "source": {
        "include": [
            "./"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "tutorials": "./tutorials",
        "template": "node_modules/tui-jsdoc-template"
    },
    "templates": {
        "tabNames": {
            "api": "Docs",
            "tutorials": "Tutorials"
        },
        "logo": {
            "url": "http://www.keyingkids.com/images/ChaosHead-typing.gif",
            "width": "150px",
            "height": "150px",
            "link": "https://github.com/nhnent/tui.jsdoc-template"
        }, 
        "css": [
            "styles/custom.css"
        ]
    }
}